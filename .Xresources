!-- Xft settings -- !
Xft.dpi:                96
Xft.antialias:          true
Xft.rgba:               rgb
Xft.hinting:            true
Xft.hintstyle:          hintfull

URxvt.font:             xft:Fantasque Sans Mono:Regular:size=11
URxvt.boldFont:         xft:Fantasque Sans Mono:Bold:size=11
URxvt.italicFont:       xft:Fantasque Sans Mono:Italic:size=11
URxvt.boldItalicFont:   xft:Fantasque Sans Mono:Bold:Italic:size=11
URxvt.letterSpace:      -1
URxvt.scrollBar:        false

